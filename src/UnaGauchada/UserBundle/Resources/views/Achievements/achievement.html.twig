<tr>
    <td data-toggle="popover" data-placement="left" data-content="El nombre que elegiste ya esta siendo usado. Selecciona un nuevo nombre">
        <div class="form-group">
            <input placeholder="Introduzca el nombre del Logro" class="campoNombre" type="text" value="Buen Tipo">
        </div>
    </td>
    <td class="text-center" data-toggle="popover" data-placement="left" data-content="Ingresaste un valor fuera del rango Permitido.">
        <div class="form-group">
            <input placeholder="Introduzca el valor mÃ¡ximo" type="number" class="campoRango" value="1">
        </div>
    </td>
    <td class="text-center">
        <button type="button" class="btn btn-secondary btn-icon"></button>
        <input type="text" class="icono" value="fa-cog" spellcheck="false">
    </td>
    <td class="text-center">
        <div class="btn-group editable" role="group">
            <button type="button" class=" btn btn-info btn-sm table-up">
                <i class="fa fa-arrow-up" aria-hidden="true"></i>
            </button>
            <button type="button" class="btn btn-info btn-sm table-down">
                <i class="fa fa-arrow-down" aria-hidden="true"></i>
            </button>
            <button type="button" class="btn btn-success btn-sm submit" data-toggle="tooltip" data-placement="left" title="Aceptar">
                <i class="fa fa-check" aria-hidden="true"></i>
            </button>
            <button type="reset" class="editCancel btn btn-danger btn-sm" data-toggle="tooltip" data-placement="right" title="Cancelar">
                <i class="fa fa-times" aria-hidden="true"></i>
            </button>
        </div>
        <div class="btn-group botones" role="group">
            <button type="button" class="edit btn btn-primary btn-sm" data-toggle="tooltip" data-placement="left" title="Editar">
                <i class="fa fa-pencil" aria-hidden="true"></i>
            </button>
            <button type="button" class="btn btn-danger btn-sm table-remove" data-toggle="tooltip" data-placement="right" title="Eliminar">
                <i class="fa fa-trash" aria-hidden="true"></i>
            </button>
        </div>
    </td>
</tr>
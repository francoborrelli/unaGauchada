{% embed 'utils/card.html.twig' with {'title': 'Registrarme'} %} 
{% block content %}
<div class="container">
<form>
    <div class="form-group row">
        <div class="col-xs-12 col-md-6">
        <label for="exampleInputEmail1">Nombre</label> 
        <input type="text" class="form-control" id="inlineFormInput" placeholder="Nombre">
        </div>
        <div class="col-xs-12 col-md-6">
        <label for="exampleInputEmail1">Apellido</label> 
        <input type="text" class="form-control" id="inlineFormInput" placeholder="Apellido">
        </div>
    </div>
    <label for="exampleInputEmail1">Email</label>
    <div class="form-group"> <input type="email" class="form-control" id="formGroupExampleInput" placeholder="Dirección de email"> 
    </div>
    <label for="exampleInputEmail1">Contraseña</label>
    <div class="form-group row"> 
        <div class="col-xs-12 col-md-6">
        <input type="password" class="form-control" id="inlineFormInput" placeholder="Contraseña">
        </div>
        <div class="col-xs-12 col-md-6">
        <input type="password" class="form-control" id="inlineFormInput" placeholder="Confirmar Contraseña">
        </div>
    </div>    
    <label for="exampleInputEmail1">Fecha de Nacimiento</label>
    <div class="form-group"> <input type="date" class="form-control" id="formGroupExampleInput2" placeholder="Fecha de Nacimiento"> </div>
    <label for="exampleInputEmail1">Teléfono</label>
    <div class="form-group"> <input type="tel" class="form-control" id="formGroupExampleInput2" placeholder="teléfono"> </div> 
    <div class="form-group">
    </div> 
    <button class="form-group btn btn-lg btn-primary btn-block" type="submit">Registrame</button>
</form> 
</div>
{% endblock %} 
{% endembed %}